@{
    Layout = null;
}



<!DOCTYPE html>
<html lang="zxx">

<head>
    <meta charset="UTF-8">
    <meta name="description" content="DJoz Template">
    <meta name="keywords" content="DJoz, unica, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>DJoz | Template</title>

    <!-- Google Font -->
    <link href="https://fonts.googleapis.com/css2?family=Rajdhani:wght@400;500;600;700&display=swap" rel="stylesheet">

    <!-- Css Styles -->
    <link rel="stylesheet" href="~/music2/css/bootstrap.min.css" type="text/css">
    <link rel="stylesheet" href="~/music2/css/font-awesome.min.css" type="text/css">
    <link rel="stylesheet" href="~/music2/css/barfiller.css" type="text/css">
    <link rel="stylesheet" href="~/music2/css/nowfont.css" type="text/css">
    <link rel="stylesheet" href="~/music2/css/rockville.css" type="text/css">
    <link rel="stylesheet" href="~/music2/css/magnific-popup.css" type="text/css">
    <link rel="stylesheet" href="~/music2/css/owl.carousel.min.css" type="text/css">
    <link rel="stylesheet" href="~/music2/css/slicknav.min.css" type="text/css">
    <link rel="stylesheet" href="~/music2/css/style.css" type="text/css">
</head>

<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header Section Begin -->
    <header class="header">
        <div class="container">
            <div class="row">
                <div class="col-lg-2 col-md-2">
                    <div class="header__logo">
                        <a href="./index.html"><img src="~/music2/img/retro5.png" style="height:100px;width:100px;" alt=""></a>
                    </div>
                </div>
                <div class="col-lg-10 col-md-10">
                    <div class="header__nav">
                        <nav class="header__menu mobile-menu">
                            <ul>
                                <li class="active"><a href="/Music/Home/">Ana Sayfa</a></li>
                                <li><a href="/Music/Search/">Arama</a></li>
                                <li><a href="/Music/Recommendation/">Tavsiye</a></li>
                            </ul>
                        </nav>
                        <div class="header__right__social">
                            <a href="#"><i class="fa fa-facebook"></i></a>
                            <a href="#"><i class="fa fa-twitter"></i></a>
                            <a href="#"><i class="fa fa-instagram"></i></a>
                            <a href="#"><i class="fa fa-dribbble"></i></a>
                        </div>
                    </div>
                </div>
            </div>
            <div id="mobile-menu-wrap"></div>
        </div>
    </header>
    <!-- Header Section End -->

    @RenderBody()



    <br />
    <!-- Footer Section Begin -->
    <footer class="footer spad set-bg" style="margin-top:1px;" data-setbg="~/music2/img/footer-bg.png">
        <div class="container">
            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
            <div class="footer__copyright__text">
                <p>İrem Ertürk tarafından tasarlanmıştır &copy; <script>document.write(new Date().getFullYear());</script> Tüm Hakları Bize Aittir. </p>
            </div>
            <!-- Link back to Colorlib can't be removed. Template is licensed under CC BY 3.0. -->
        </div>
    </footer>
    <!-- Footer Section End -->
    <!-- Js Plugins -->
    <script src="~/music2/js/jquery-3.3.1.min.js"></script>
    <script src="~/music2/js/bootstrap.min.js"></script>
    <script src="~/music2/js/jquery.magnific-popup.min.js"></script>
    <script src="~/music2/js/jquery.nicescroll.min.js"></script>
    <script src="~/music2/js/jquery.barfiller.js"></script>
    <script src="~/music2/js/jquery.countdown.min.js"></script>
    <script src="~/music2/js/jquery.slicknav.js"></script>
    <script src="~/music2/js/owl.carousel.min.js"></script>
    <script src="~/music2/js/main.js"></script>

    <!-- Music Plugin -->
    <script src="~/music2/js/jquery.jplayer.min.js"></script>
    <script src="~/music2/js/jplayerInit.js"></script>
</body>

</html>
